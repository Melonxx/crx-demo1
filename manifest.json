{
  "manifest_version": 2,
  "name": "肥肥",
  "short_name": "肥",
  "description": "这是我的第一个ctx应用",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "images/jjb.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*.alimama.com/*"],
      "js": [
        "jquery.min.js",
        "1.js"
      ],
      "css": [
        "style.css"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';",
  "icons": {
    "16": "images/jjb.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "permissions": [
    "*://*.alimama.com/*"
  ],
  "optional_permissions": [
    "*://*.jdpay.com/*",
    "bookmarks"
  ]
}
